{
    "name": "nano",
    "description": "A minimalist web proxy built to be simple yet powerful.",
    "contributors": [
        "Leaflet",
        "Nebelung"
    ],
    "version": "1.0.0",
    "license": "AGPL-3.0-only",
    "type": "module",
    "packageManager": "pnpm@10.14.0",
    "scripts": {
        "start": "node index.js",
        "build": "vite build",
        "build-static": "vite build && node build.js",
