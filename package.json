    },
    "dependencies": {
        "@mercuryworkshop/wisp-js": "0.3.3",
        "chemicaljs": "^2.3.1",
        "dreamland": "^0.0.24",
        "dreamland-router": "https://github.com/mercuryworkshop/dreamland-router",
        "express": "^4.19.2",
        "sortablejs": "^1.15.2",
        "sqlite3": "^5.1.7",
        "node-gyp-build": "^4.8.4"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.42",
        "prettier": "^3.3.3",
        "rollup-plugin-typescript2": "^0.36.0",
        "tailwindcss": "^3.4.10",
        "vite": "^5.4.2",
        "vite-plugin-dreamland": "^1.2.1"
    },
    "engines": {
        "node": ">=20.11.0"
    },
"pnpm": {
  "overrides": {
    "chemicaljs": "2.3.1"
  },
  "onlyBuiltDependencies": [
    "bufferutil",
    "esbuild",
    "sqlite3",
    "node-gyp-build"
  ]
}

}
